{
    "name": "jest-typescript-toolbox",
    "displayName": "Jest TypeScript Toolbox",
    "description": "",
    "version": "1.0.0",
    "publisher": "svenliebig",
    "author": {
        "email": "liebigsv@gmail.com",
        "name": "Sven Liebig",
        "url": "http://svenliebig.com"
    },
    "bugs": {
        "url": "https://github.com/Sly321/jest-typescript/issues"
    },
    "repository": {
        "url": "https://github.com/Sly321/jest-typescript-toolbox"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other",
        "Programming Languages",
        "Debuggers"
    ],
    "keywords": [
        "jest",
        "typescript",
        "test",
        "react",
        "tsx"
    ],
    "homepage": "https://github.com/Sly321/jest-typescript",
    "icon": "assets/logo.png",
    "galleryBanner": {
        "color": "#007AC9",
        "theme": "dark"
    },
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:typescriptreact",
        "onView:jestExplorer"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "jestTypescriptToolbox.reloadTree",
                "title": "Reload Tree",
                "category": "jest-typescript-toolbox",
                "icon": "assets/jest.svg"
            }
        ],
        "menus": {
            "view/item/context": [
                {
                    "command": "jestTypescriptToolbox.reloadTree",
                    "when": "viewItem == jestExplorerDescribeNode"
                }
            ],
            "commandPalette": []
        },
        "views": {
            "jestView": [
                {
                    "id": "jestExplorer",
                    "name": "Explorer"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "jestView",
                    "title": "Jest Tree",
                    "icon": "assets/jest.svg"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "publish": "vsce publish",
        "patch": "vsce publish patch",
        "minor": "vsce publish minor",
        "major": "vsce publish major"
    },
    "devDependencies": {
        "@types/node": "^7.0.43",
        "tslint": "5.10.0",
        "vscode": "1.1.18"
    },
    "dependencies": {
        "typescript": "2.9.1",
        "jest": "^23.1.0"
    }
}