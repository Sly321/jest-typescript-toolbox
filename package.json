{
    "name": "jest-typescript-toolbox",
    "displayName": "Jest TypeScript Toolbox",
    "description": "",
    "version": "0.0.1",
    "publisher": "svenliebig",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onLanguage:typescript",
        "onLanguage:typescriptreact",
        "onView:jestExplorer"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }],
        "views": {
            "jestView": [{
                "id": "jestExplorer",
                "name": "Explorer"
            }]
        },
        "viewsContainers": {
            "activitybar": [{
                "id": "jestView",
                "title": "Jest Tree",
                "icon": "assets/jest.svg"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}